  camel-snake-kebab/camel-snake-kebab   {:mvn/version "0.4.3"}
  instaparse/instaparse                 {:mvn/version "1.4.10"}
  org.clojars.mmb90/cljs-cache          {:mvn/version "0.1.4"}
  fipp/fipp                             {:mvn/version "0.6.26"}
